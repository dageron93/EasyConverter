<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="boroda.com.easyconverter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EasyConverter"
        tools:ignore="AllowBackup">
        <activity
            android:name="boroda.com.easyconverter.AMenuActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="boroda.com.easyconverter.LengthMain"
            android:label="@string/length_activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="boroda.com.easyconverter.MassMain"
            android:label="@string/mass_activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="boroda.com.easyconverter.TemperatureMain"
            android:label="@string/temperature_activity"
            android:screenOrientation="portrait" />
    </application>


</manifest>